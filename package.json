{
  "$schema": "https://enconvo.com/schemas/extension.json",
  "name": "tts_providers",
  "version": "0.0.100",
  "description": "Text to Speech Providers",
  "title": "TTS Providers",
  "icon": "icon.png",
  "author": "EnconvoAI",
  "categories": [
    "Provider"
  ],
  "license": "MIT",
  "type": "module",
  "commands": [
    {
      "name": "edge_tts_provider",
      "title": "Edge TTS",
      "description": "Edge TTS Provider that allows you to use Microsoft Edge's online text-to-speech service",
      "icon": "edge_tts.jpg",
      "mode": "provider",
      "preferences": [
        {
          "name": "voice",
          "title": "Speech Voice",
          "description": "The voice to TTS.",
          "type": "dropdown",
          "required": false,
          "dataProxy": "tts_providers|edge_voices",
          "default": "en-US-EmmaMultilingualNeural"
        },
        {
          "name": "speed",
          "title": "Speech Speed",
          "description": "The speed to TTS.",
          "type": "dropdown",
          "required": false,
          "dataProxy": "tts_providers|voice_speed_list",
          "default": "1.2"
        }
      ]
    },
    {
      "name": "enconvo_mirosoft_tts_provider",
      "title": "Enconvo Cloud Plan( Microsoft TTS )",
      "description": "Microsoft TTS Provider that allows you to use Microsoft Azure's online text-to-speech service, provided by Enconvo Cloud Plan",
      "icon": "enconvo.png",
      "mode": "provider",
      "preferences": [
        {
          "name": "voice",
          "title": "Speech Voice",
          "description": "The voice to TTS.",
          "type": "dropdown",
          "required": false,
          "dataProxy": "tts_providers|azure_voices",
          "default": "en-US-EmmaMultilingualNeural"
        },
        {
          "name": "speed",
          "title": "Speech Speed",
          "description": "The speed to TTS.",
          "type": "dropdown",
          "required": false,
          "dataProxy": "tts_providers|voice_speed_list",
          "default": "1.2"
        }
      ]
    },
    {
      "name": "edge_voices",
      "title": "edge_voices",
      "description": "get edge_voices list",
      "icon": "icon.png",
      "showInCommandList": false,
      "mode": "no-view"
    },
    {
      "name": "mirosoft_tts_provider",
      "title": "Microsoft TTS",
      "description": "Microsoft TTS Provider that allows you to use [Microsoft Azure's online text-to-speech](https://learn.microsoft.com/en-us/azure/ai-services/speech-service/get-started-text-to-speech?tabs=macos%2Cterminal&pivots=programming-language-rest) service",
      "icon": "azure.png",
      "mode": "provider",
      "preferences": [
        {
          "name": "voice",
          "title": "Speech Voice",
          "description": "The voice to TTS.",
          "type": "dropdown",
          "required": false,
          "dataProxy": "tts_providers|azure_voices",
          "default": "en-US-EmmaMultilingualNeural"
        },
        {
          "name": "speed",
          "title": "Speech Speed",
          "description": "The speed to TTS.",
          "type": "dropdown",
          "required": false,
          "dataProxy": "tts_providers|voice_speed_list",
          "default": "1.2"
        },
        {
          "name": "region",
          "title": "Resource Region",
          "description": "The region of the resource.",
          "type": "dropdown",
          "required": true,
          "default": "eastus",
          "defaultProxy": "KEY_AZURE_SPEECH_REGION",
          "dataProxy": "speech_recognize_providers|azure_speech_regions"
        },
        {
          "name": "resource_key",
          "title": "Resource Key",
          "description": "How to get api key? [ðŸ”‘here](https://learn.microsoft.com/en-us/azure/ai-services/speech-service/overview)",
          "type": "password",
          "required": true,
          "default": "",
          "defaultProxy": "KEY_AZURE_SPEECH",
          "placeholder": "********"
        }
      ]
    },
    {
      "name": "azure_voices",
      "title": "azure_voices",
      "commandType": "function_command",
      "description": "get azure_voices list",
      "icon": "icon.png",
      "showInCommandList": false,
      "mode": "no-view"
    },
    {
      "name": "voice_speed_list",
      "title": "voice_speed_list",
      "commandType": "function_command",
      "description": "get voice_speed_list list",
      "icon": "icon.png",
      "showInCommandList": false,
      "mode": "no-view"
    },
    {
      "name": "openai_tts_provider",
      "title": "OpenAI TTS",
      "description": "OpenAI TTS Provider that allows you to use [OpenAI's online text-to-speech](https://platform.openai.com/docs/guides/text-to-speech) service",
      "icon": "openai.png",
      "mode": "provider",
      "preferences": [
        {
          "name": "openAIApiKey",
          "description": "OpenAI api key",
          "type": "password",
          "required": false,
          "title": "OpenAI Api Key",
          "default": "",
          "defaultProxy": "KEY_OPENAI_APIKEY",
          "placeholder": "OpenAI Api Key"
        },
        {
          "name": "baseUrl",
          "description": "OpenAI api base url",
          "type": "textfield",
          "required": false,
          "title": "OpenAI Api Base Url",
          "default": "https://api.openai.com/v1",
          "defaultProxy": "KEY_OPENAI_BASEURL",
          "placeholder": "htts://api.openai.com/v1"
        },
        {
          "name": "modelName",
          "description": "The model to TTS.",
          "type": "dropdown",
          "required": false,
          "title": "Model Name",
          "default": "tts-1",
          "data": [
            {
              "title": "tts-1",
              "value": "tts-1"
            },
            {
              "title": "tts-1-hd",
              "value": "tts-1-hd"
            }
          ]
        },
        {
          "name": "voice",
          "description": "The voice to TTS.",
          "type": "dropdown",
          "required": false,
          "title": "Voice",
          "default": "alloy",
          "data": [
            {
              "title": "alloy",
              "value": "alloy"
            },
            {
              "title": "echo",
              "value": "echo"
            },
            {
              "title": "fable",
              "value": "fable"
            },
            {
              "title": "onyx",
              "value": "onyx"
            },
            {
              "title": "nova",
              "value": "nova"
            },
            {
              "title": "shimmer",
              "value": "shimmer"
            }
          ]
        },
        {
          "name": "speed",
          "title": "Speech Speed",
          "description": "The speed to TTS.",
          "type": "dropdown",
          "required": false,
          "dataProxy": "tts_providers|voice_speed_list",
          "default": "1.2"
        }
      ]
    },
    {
      "name": "google_tts_provider",
      "title": "Google TTS",
      "description": "Google TTS Provider that allows you to use [Google's online text-to-speech](https://cloud.google.com/text-to-speech/) service",
      "icon": "google.png",
      "mode": "provider",
      "preferences": [
        {
          "name": "apiKey",
          "description": "Google api key",
          "type": "password",
          "required": false,
          "title": "Google Api Key",
          "default": "",
          "defaultProxy": "KEY_GOOGLE_APIKEY",
          "placeholder": "AI*****"
        },
        {
          "name": "voice",
          "description": "The voice to TTS.",
          "type": "dropdown",
          "required": false,
          "title": "Voice",
          "default": "en-US-Journey-D",
          "dataProxy": "tts_providers|google_voices"
        },
        {
          "name": "speed",
          "title": "Speech Speed",
          "description": "The speed to TTS.",
          "type": "dropdown",
          "required": false,
          "dataProxy": "tts_providers|voice_speed_list",
          "default": "1.2"
        }
      ]
    },
    {
      "name": "system_tts_provider",
      "title": "System TTS",
      "description": "System TTS Provider that allows you to use `say` command on macos",
      "icon": "system.png",
      "mode": "provider",
      "preferences": [
        {
          "name": "voice",
          "description": "The voice to TTS.",
          "type": "dropdown",
          "required": false,
          "title": "Voice",
          "default": "",
          "dataProxy": "tts_providers|system_voices"
        },
        {
          "name": "speed",
          "title": "Speech Speed",
          "description": "The speed to TTS.",
          "type": "dropdown",
          "required": false,
          "dataProxy": "tts_providers|voice_speed_list",
          "default": "1.2"
        },
        {
          "name": "format",
          "description": "The format to TTS.",
          "type": "textfield",
          "required": false,
          "title": "Format",
          "default": "m4a",
          "visibility": "hidden"
        }
      ]
    },
    {
      "name": "google_voices",
      "title": "Google Voices",
      "description": "get google_voices list",
      "icon": "icon.png",
      "commandType": "function_command",
      "showInCommandList": false,
      "mode": "no-view",
      "preferences": [
        {
          "name": "api_key",
          "description": "Google Api Key",
          "type": "password",
          "required": false,
          "title": "Api Key",
          "default": "",
          "defaultProxy": "KEY_GOOGLE_APIKEY",
          "placeholder": "AI****"
        }
      ]
    },
    {
      "name": "system_voices",
      "title": "System Voices",
      "description": "get system voices list",
      "icon": "icon.png",
      "commandType": "function_command",
      "showInCommandList": false,
      "mode": "no-view"
    },
    {
      "name": "elevenlabs_voices",
      "title": "elevenlabs_voices",
      "description": "get elevenlabs_voices list",
      "icon": "icon.png",
      "commandType": "function_command",
      "showInCommandList": false,
      "mode": "no-view",
      "preferences": [
        {
          "name": "api_key",
          "description": "Api Key",
          "type": "password",
          "required": false,
          "title": "Api Key",
          "default": "",
          "defaultProxy": "tts_providers|elevenlabs_tts_provider",
          "placeholder": "ElevenLabs Api Key"
        },
        {
          "name": "baseUrl",
          "description": "ElevenLabs api base url",
          "type": "textfield",
          "required": false,
          "title": "ElevenLabs Api Base Url",
          "default": "https://api.elevenlabs.io/v1",
          "placeholder": "ElevenLabs Api Base Url"
        }
      ]
    },
    {
      "name": "elevenlabs_models",
      "title": "elevenlabs_models",
      "description": "get elevenlabs_models list",
      "icon": "icon.png",
      "commandType": "function_command",
      "showInCommandList": false,
      "mode": "no-view",
      "preferences": [
        {
          "name": "api_key",
          "description": "Api Key",
          "type": "password",
          "required": false,
          "title": "Api Key",
          "default": "",
          "defaultProxy": "tts_providers|elevenlabs_tts_provider",
          "placeholder": "ElevenLabs Api Key"
        },
        {
          "name": "baseUrl",
          "description": "ElevenLabs api base url",
          "type": "textfield",
          "required": false,
          "title": "ElevenLabs Api Base Url",
          "default": "https://api.elevenlabs.io/v1",
          "placeholder": "ElevenLabs Api Base Url"
        }
      ]
    }
  ],
  "dependencies": {
    "@enconvo/api": "^0.1.161",
    "@google-cloud/text-to-speech": "^5.6.0",
    "axios": "^1.7.9",
    "file-system-cache": "^3.0.0",
    "microsoft-cognitiveservices-speech-sdk": "^1.41.0",
    "msedge-tts": "workspace:*",
    "openai": "^4.76.0"
  },
  "devDependencies": {
    "@types/node": "^18.19.67",
    "eslint": "^8.57.1",
    "prettier": "^2.8.8",
    "tsup": "^7.2.0",
    "typescript": "^5.7.2"
  },
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "npm run lint --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --list-different \"**/*.ts\"",
    "build": "enconvo",
    "dev": "enconvo --dev"
  },
  "minAppVersion": "1.8.8"
}
